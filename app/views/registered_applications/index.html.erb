<h1>All Registered Applications</h1>

 <div class="row">
   <div class="col-md-8">
     <% @registered_applications.each do |registered| %>
       <div class="media">
         <div class="media-body">
           <h4 class="media-heading">
             <%= link_to registered.name, registered %>
           </h4>
           <small>
             <%= registered.url %>
           </small>
         </div>
       </div>
     <% end %>
   </div>
   <div class="col-md-4">
     <%= link_to "Register A New Application", new_registered_application_path, class: 'btn btn-success' %>
   </div>
 </div
