
<h1><%= @registered_application.name %></h1>
 <div class="row">
   <div class="col-md-8">
     <p><%= @registered_application.url %></p>
   </div>
   <div class="col-md-4">
     <%= link_to "Edit Your Registered Application", edit_registered_application_path(@registered_application), class: 'btn btn-success' %>
     <%= link_to "Delete Your Registered Application", @registered_application, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete your registration?'} %>
   </div>
 </div>
